
<section class="featued-bannar">
<div class="container">
 <div class="row">
    {% for block in section.blocks %}
        <div class="col-md-6">
          <div class="featued-bannar-wrapper">
            <div class="featued-bannar-img">
               <img src="{{ block.settings.img | img_url:'' }}" alt="">
            </div>
            <div class="featued-bannar-content">
              <h2>{{ block.settings.h2 }}</h2>
              <a href="{{ block.settings.btn_link }}" class="btn">{{block.settings.btn}}</a>
            </div>
         </div>

        </div>
     {% endfor %}



 </div>
 </div>


</section>



{% schema %}
    {
        "name": "Featued Bannar 01",
        "class": "bannar-01",
        "blocks": [
            {
                "type": "bannar",
                "name": "Featued Bannar",
                "settings": [
                    {
                        "type": "image_picker",
                        "id": "img",
                        "label": "Bannar image"
                    },
                    {
                        "type": "text",
                        "id": "h3",
                        "label": " Bannar Title"
                    },
                    {
                        "type": "text",
                        "id": "btn",
                        "label": " Bannar Title"
                    },
                    {
                        "type": "url",
                        "id": "btn_link",
                        "label": " Bannar Button link"
                    }
                ]
            }
        ],
        
        "presets": [
            {
                "name": "Featued Bannar 01"
            }
        ]
    }
{% endschema %}
